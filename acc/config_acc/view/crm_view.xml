<?xml version="1.0"?>
<odoo>

<data>
    <record id="crm_lead_view_tree_acc" model="ir.ui.view">
        <field name="model">crm.lead</field>
        <field name="name">crm.lead.tree.acc</field>
        <field name="inherit_id" ref="crm.crm_case_tree_view_oppor"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='activity_ids']" position="after">
				<field name="products_node_ids" widget='many2many_tags' optional="show"/>
				<field name="node_zone_localidad_id" optional="show"/>
				<field name="node_nodo_arsat" optional="show"/>
				<field name="estado_contrato" optional="show"/>
				<field name="project_stage_id" optional="show"/>
				<field name="ticket_jira" optional="hide"/>
			</xpath>
		</field>
	</record>
	
	        <record id="crm_lead_view_form_acc" model="ir.ui.view">
            <field name="name">crm.lead.form.acc</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_lead_view_form"/>
            <field name="arch" type="xml">
				<field name="tag_ids" position='after'>
					
                    <field name="motivo_no_factibilidad_id"/>
                    <field name="tipo_factibilidad" widget='radio'/>
                    <field name="presentada_gca"/>
                    <field name="ticket_jira"/>
				</field>
			</field>
        </record>
</data>
