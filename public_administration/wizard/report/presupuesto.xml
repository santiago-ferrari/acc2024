<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="pa_report_presupuesto" model="ir.ui.view">
            <field name="name">Reporte de Contabilidad</field>
            <field name="model">pa.wizard_report_presupuesto</field>
            <field name="arch" type="xml">
                <form string="Reporte de contabilidad presupuestaria">
                    <group col="4">
                        <field name="reporte"/>
                        <field name="budget_id" options="{'no_create_edit': True}"/>
                        <field name="date_end"/>
                    </group>
                    <!-- group col="2">
                        <field name="accounts_tags_niveles_ids" widget="many2many_tags" options="{'no_create_edit': True}"/>
                    </group -->
                    <footer>  
                        <button name="print_report_presupuesto" type="object" string="Reporte a pdf" class="oe_highlight"/>
                        <!-- button string='Reporte a excel' name="download_file_lf" type="object" class="oe_highlight"/ -->  
                        <button string="Cancelar" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</openerp>